<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>netstandard2.1</TargetFramework>
		<Nullable>enable</Nullable>
		<!-- This should be the folder that contains Duckov.exe -->
		<!-- 下面这行里填写你的鸭科夫安装路径，这样编译器就能找到相关的dll文件 -->
		<DuckovPath>E:\Steam\steamapps\common\Escape from Duckov\Duckov_Data</DuckovPath>
		
	</PropertyGroup>
	<ItemGroup>
		<Reference Include="$(DuckovPath)\Managed\TeamSoda.*" />
		<Reference Include="$(DuckovPath)\Managed\ItemStatsSystem.dll" />
		<Reference Include="$(DuckovPath)\Managed\Unity*" />
	</ItemGroup>
</Project>
